# Optional. If unset, the app auto-detects the dev server host for local development.
# EXPO_PUBLIC_API_URL=http://192.168.1.15:8000/api/v1
EXPO_PUBLIC_API_URL=

# Hide placeholder social auth buttons unless you implement them.
EXPO_PUBLIC_SOCIAL_AUTH_ENABLED=false

# Google Sign-In (optional)
# Create OAuth client IDs in Google Cloud Console and fill in the relevant ones.
# - Web client ID is typically used for Expo Go / web and can work as a fallback.
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=
